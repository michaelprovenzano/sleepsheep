{
  "name": "sleep-tracker",
  "version": "1.0.0",
  "description": "A simple app for assessing the quality of sleep",
  "main": "server.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run watch:js\" \"npm run watch:css\" \"npm run start:dev\"",
    "start:dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:css": "node-sass -rw dev_data/html/sass/main.scss public/css/style.css -w",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public --out-file bundle.js"
  },
  "author": "Michael Provenzano",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "aos": "^2.3.4",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.2.5",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.18",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pickadate": "^5.0.0-alpha.3",
    "pug": "^2.0.4",
    "slugify": "^1.4.6",
    "validator": "^12.2.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "ndb": "^1.1.5",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4"
  }
}
